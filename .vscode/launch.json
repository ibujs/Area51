{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Next.js",
            "port": 9229,
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Next.js <experimental>",
            "program": "${workspaceRoot}/node_modules/.bin/next"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:3000",
            // Remove the // in the next lines to be able to use alternative chrome-based browsers by modifying "runtimeExecutable"
            "webRoot": "${workspaceRoot}"//,
            //"runtimeExecutable": "chrome"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}",
            "port": 9222
        }
    ]
}
